{
  "type": "module",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development conc -n client,server \"npm run start:client\" \"npm run start:server\"",
    "build": "NODE_ENV=production npm run build:client && npm run build:server",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write src",
    "start:client": "vite",
    "start:server": "tsx --watch src/server/index.ts",
    "build:client": "vite build",
    "build:server": "rm -rf build/{config,server} && tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "env:encrypt:dev": "dotenvx encrypt -f .env.dev",
    "env:encrypt:prod": "dotenvx encrypt -f .env.prod",
    "env:decrypt:dev": "dotenvx decrypt -f .env.dev",
    "env:decrypt:prod": "dotenvx decrypt -f .env.prod",
    "cli": "tsx src/server/cli.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "1.52.0",
    "@hono/node-server": "1.19.9",
    "@sequelize/core": "7.0.0-alpha.48",
    "@sequelize/mysql": "7.0.0-alpha.48",
    "@sequelize/sqlite3": "7.0.0-alpha.48",
    "hono": "4.12.0",
    "node-cron": "^4.2.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router-dom": "7.13.0",
    "socks": "2.8.7",
    "styled-components": "6.3.11",
    "undici": "7.22.0",
    "webtorrent": "2.8.5",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "25.3.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/webtorrent": "0.110.1",
    "@vitejs/plugin-react": "5.1.4",
    "babel-plugin-styled-components": "2.1.4",
    "concurrently": "9.2.1",
    "jsdom": "^28.1.0",
    "prettier": "3.8.1",
    "tsc-alias": "1.8.16",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.1.1",
    "vitest": "^4.0.18"
  }
}
